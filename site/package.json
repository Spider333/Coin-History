{
  "name": "coin-history",
  "version": "1.0.0",
  "description": "Crypto resource lists",
  "author": "Dave Stancel",
  "private": true,
  "scripts": {
    "dev": "cp ../README.md ./static/resources.md && nuxt",
    "build": "cp ../README.md ./static/resources.md && nuxt build",
    "start": "cp ../README.md ./static/resources.md && nuxt start",
    "generate": "cp ../README.md ./static/resources.md && nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "fix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "generate:gh-pages": "cp ../README.md ./static/resources.md && DEPLOY_ENV=GH_PAGES nuxt generate",
    "deploy:gh-pages": "push-dir --dir=dist --branch=gh-pages --cleanup"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/pwa": "^3.0.0-0",
    "nuxt": "^2.0.0",
    "vue-markdown": "^2.2.4"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "push-dir": "^0.4.1",
    "raw-loader": "^3.1.0",
    "eslint-plugin-nuxt": ">=0.4.2"
  }
}
